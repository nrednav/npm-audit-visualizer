{
  "name": "npm-audit-visualizer/web-app",
  "version": "1.0.0-alpha.6",
  "description": "The web-app component of npm-audit-visualizer",
  "author": "Vandern Rodrigues <nrednav@gmail.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "predev": "cp ../../../build/web-app/parsed-audit-report.json ./public/parsed-audit-report.json",
    "dev": "vite --open",
    "prebuild": "npm run clean",
    "build": "npx tsc && vite build",
    "clean": "npx del ./build/ && npx del ./public/parsed-audit-report.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@react-sigma/core": "^4.0.2",
    "clsx": "^2.1.0",
    "graphology": "^0.25.4",
    "graphology-layout": "^0.6.1",
    "graphology-types": "^0.24.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sigma": "^3.0.0-beta.17"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.3.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.11.6",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "del-cli": "^5.1.0",
    "eslint": "^8.55.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-testing-library": "^6.2.0",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^1.2.1"
  }
}
